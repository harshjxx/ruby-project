<p id="notice"><%= notice %></p>

<h1>Employee Tables</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_tables.each do |employee_table| %>
      <tr>
        <td><%= employee_table.name %></td>
        <td><%= employee_table.email %></td>
        <td><%= employee_table.address %></td>
        <td><%= link_to 'Show', employee_table %></td>
        <td><%= link_to 'Edit', edit_employee_table_path(employee_table) %></td>
        <td><%= link_to 'Destroy', employee_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee Table', new_employee_table_path %>
